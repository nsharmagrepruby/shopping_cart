<p id='alert'><%= alert %></p>
<h1>Login</h1>
  <%= form_with model: @user, url: sessions_path, local: true do |form| %>
    <p>
      <%= form.label :email %>
      <%= form.text_field :email %>
    </p>

    <p>
    <%= form.label :password %>
    <%= form.password_field :password %>
    </p>
    <%= form.submit 'login' %>
<% end %>